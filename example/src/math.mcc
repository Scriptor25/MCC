namespace example

define :add(x, y) #:math {
    result = x + y
    print(@s, `${x} + ${y} = ${result}`)
    return result
}

define :sub(x, y) #:math {
    result = x - y
    print(@s, `${x} - ${y} = ${result}`)
    return result
}

define :fib(n) {
    args = { n: n }
    return if (n <= 1) n
    else $:fib_else(args)
}

define :fib_else(n) {
    return $:fib({ n: n - 1 }) + $:fib({ n: n - 2 })
}
