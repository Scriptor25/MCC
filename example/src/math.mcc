namespace example

define add(x: number, y: number): number #math {
    const result = x + y
    !print("@s", `${x} + ${y} = ${result}`)
    return result
}

define sub(x: number, y: number): number #math {
    const result = x - y
    !print("@s", `${x} - ${y} = ${result}`)
    return result
}

define fib(n: number): number {
    if (n <= 1)
        return n
    return fib(n - 1) + fib(n - 2)
}

define test_math() {
    add(42, 17)
    sub(42, 17)
}
